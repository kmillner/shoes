<h1>Something Ain't Right With The Store? UPDATEEEE IT! </h1>

<form action="/stores/$store.getId()/update" method="post">
  <label for="name">Store Name</label>
  <input id="name" name="name" value="$store.getName()" type="text">
  <label for="price">Store Price Range</label>
    <input id="price" placeholder="$store.getPrice()" name="price" type="text">
  <button type="submit" class="btn btn-success">Submit</button>
</form>
